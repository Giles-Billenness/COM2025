= form_for @build do |f|
  - if @build.errors.any?
    #error_explanation
      %h2= "#{pluralize(@build.errors.count, "error")} prohibited this build from being saved:"
      %ul
        - @build.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :title
    = f.text_field :title

  .field

    = collection_select( :build, :cpu_id, CentralProcessingUnit.all, :id, :ChipModel  , {}, { :multiple => false } )
  .actions
    = f.submit 'Save'
